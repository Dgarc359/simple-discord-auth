  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": "https://github.com/Dgarc359/simple-discord-auth.git",
  "author": "David Garcia <87497958+Dgarc359@users.noreply.github.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "29.4.0",
    "@types/node": "18.14.0",
    "jest": "29.4.3",
    "prettier": "2.8.4",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "tslint": "6.1.3",
    "tslint-config-prettier": "1.18.0",
    "typescript": "4.9.5"
  },
  "files": [
    "lib/**/*"
  ],
  "keywords": [
    "Discord",
